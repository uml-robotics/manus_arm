<launch>

    <!-- brian_recv.py or csv_recv -->
    <arg name="receiver_node" value="brian_recv.py"/>
        
    <rosparam>
        arm_speed: 3
        arm_safe_range: 20000.0
        max_range_from_midpoint: 0.5
        loop_rate: 200
        buffer_size: 1000
        stdev_mult: 2.0
        burst_window: 1000
        brian_running_time: 21.0
        brian_connections_file_path: '/home/jon/Python/connections_2012-7-12-16:14:11.pickle'
        brian_pad_neuron_map_file_path: '/home/jon/Python/pad_2012-7-12-16:14:11.pickle'
        csv_file_path: '/home/jon/ams_thesis/data_sets/labview/no56_labviewdatafile.csv'
        burst_log_path: '/home/jon/brian_burst.csv'
        cat_log_path: '/home/jon/brian_cat.csv'
    </rosparam>
    
    <!-- Node Manager -->
    <!-- <node pkg="node_manager_fkie" name="nm" type="nm"/> -->    

    <!-- ARM Control -->   
    <node pkg="arm" name="arm_control" type="arm_control"/>
  
    <!-- Dish Visualizer -->
    <node pkg="dish_viz" name="dish_viz" type="dish_viz"/>
    
    <!-- Dish/ARM Teleop -->
    <node pkg="arm" name="teleop_arm_dish" type="teleop_arm_dish"/>
    
    <!-- CAT Creator -->
    <node pkg="burst_calc" name="cat_creator" type="cat_creator"/>

    <!-- Burst Creator -->
    <node pkg="burst_calc" name="burst_creator" type="burst_creator"/>

    <!-- Receiver Node -->
    <node pkg="neuro_recv" name="recv" type="$(arg receiver_node)"/>
        
</launch>
